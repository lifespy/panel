<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.not404.panel.domain.LinkDto" table="link" schema="sspanel">
        <id name="id" column="id"/>
        <property name="type" column="type"/>
        <property name="address" column="address"/>
        <property name="port" column="port"/>
        <property name="token" column="token"/>
        <property name="ios" column="ios"/>
        <property name="userid" column="userid"/>
        <property name="isp" column="isp"/>
        <property name="geo" column="geo"/>
        <property name="method" column="method"/>
    </class>
</hibernate-mapping>
