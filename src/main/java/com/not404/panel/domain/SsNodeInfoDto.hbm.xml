<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.not404.panel.domain.SsNodeInfoDto" table="ss_node_info" schema="sspanel">
        <id name="id" column="id"/>
        <property name="nodeId" column="node_id"/>
        <property name="uptime" column="uptime"/>
        <property name="load" column="load"/>
        <property name="logTime" column="log_time"/>
    </class>
</hibernate-mapping>
