<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.not404.panel.domain.CouponDto" table="coupon" schema="sspanel">
        <id name="id" column="id"/>
        <property name="code" column="code"/>
        <property name="onetime" column="onetime"/>
        <property name="expire" column="expire"/>
        <property name="shop" column="shop"/>
        <property name="credit" column="credit"/>
    </class>
</hibernate-mapping>
