<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.not404.panel.domain.UserTrafficLogDto" table="user_traffic_log" schema="sspanel">
        <id name="id" column="id"/>
        <property name="userId" column="user_id"/>
        <property name="u" column="u"/>
        <property name="d" column="d"/>
        <property name="nodeId" column="node_id"/>
        <property name="rate" column="rate"/>
        <property name="traffic" column="traffic"/>
        <property name="logTime" column="log_time"/>
    </class>
</hibernate-mapping>
