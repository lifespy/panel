<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.not404.panel.domain.SsPasswordResetDto" table="ss_password_reset" schema="sspanel">
        <id name="id" column="id"/>
        <property name="email" column="email"/>
        <property name="token" column="token"/>
        <property name="initTime" column="init_time"/>
        <property name="expireTime" column="expire_time"/>
    </class>
</hibernate-mapping>
